# Copy this file to .env.local and fill in the values

# RPC endpoint
NEXT_PUBLIC_RPC_URL=http://127.0.0.1:8545

# Deployer private key (server-only — never prefix with NEXT_PUBLIC_)
DEPLOYER_PRIVATE_KEY=0x<your-private-key>

# Deployer wallet address
NEXT_PUBLIC_DEPLOYER_ADDRESS=0x<your-address>

# Contract addresses — update after each fresh Anvil deployment
NEXT_PUBLIC_CONTRACT_WETH=0x<address>
NEXT_PUBLIC_CONTRACT_WBTC=0x<address>
NEXT_PUBLIC_CONTRACT_WETH_PRICE_FEED=0x<address>
NEXT_PUBLIC_CONTRACT_WBTC_PRICE_FEED=0x<address>
NEXT_PUBLIC_CONTRACT_SC_PRICE_FEED=0x<address>
NEXT_PUBLIC_CONTRACT_STABLE_COIN=0x<address>
NEXT_PUBLIC_CONTRACT_STABLE_COIN_ENGINE=0x<address>
